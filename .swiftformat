--minversion 0.47.10

# file options

--exclude Tests/LinuxMain.swift,Tests/**/XCTestManifests.swift

# format options

--commas inline
--ifdef no-indent
--importgrouping testable-bottom
--indent tab
--self init-only
--stripunusedargs closure-only
--swiftversion 5.1
--tabwidth 4

# rules

--enable isEmpty
--disable redundantReturn,trailingClosures
